package Graph;

import java.util.LinkedList;

public class AdjListGraphTest {

	public static void main(String[] args) {		
		
		int N = 4;
		AdjListGraph alg = new AdjListGraph(N);
		
		alg.buildGraph(0,1); alg.buildGraph(0,2);
		alg.buildGraph(1,0); alg.buildGraph(1,2); alg.buildGraph(1,3); 
		alg.buildGraph(2,0); alg.buildGraph(2,1); alg.buildGraph(2,3);
		
		alg.printGraph();

	}

}
