package ingTime;

public class RunningTimeMeasure {
	
	public int fConstant(int[] arr) {
		return arr.length * 2;
	}
	
	public int linearTime(int[] arr) {
		int totalSum = 0;
		for(int n : arr) {
			totalSum += n;
		}
		return totalSum;
	}
	
	public boolean fQuadratic(int[] arr) {
		int target = 30;
		for(int first : arr) {
			for(int second : arr) {
				if(first + second == target) return true;
			}
		}
		return false;
	}
	
	public boolean fCubic(int[] arr) {
		int target = 30;
		for(int first : arr) {
			for(int second : arr) {
				for(int third : arr) {
					if(first + second + third == target)
						return true;
				}
			}
		}
		return false;
	}
	
	public boolean fFourth(int[] arr) {
		int target = 30;
		for(int first : arr) {
			for(int second : arr) {
				for(int third : arr) {
					for(int four : arr) {
						if(first + second + third + four == target)
							return true;
					}
				}
			}
		}
		return false;
	}

	public static void main(String[] args) {
		
		
		int[] arr = new int[500];
		for(int i = 0; i < arr.length; i++)
			arr[i] = 0;
		
		long beforeTime = System.currentTimeMillis();
		long afterTime = System.currentTimeMillis();
		long diffTimeMilliSecond = afterTime - beforeTime;
		System.out.println("Time diff (ms): " + diffTimeMilliSecond);

	}

}
